class Animal {
    void sound() {
        System.out.println("Animal makes a sound");
    }
 }
 class Dog extends Animal {
    void sound() {
        System.out.println("Dog barks");
    }
    void show() {
        sound();         // Calls child method
        super.sound();   // Calls parent method
    }
 }
 public class Main {
    public static void main(String[] args) {
        Dog d = new Dog();
        d.show();
    }
 }
